{
  "name": "class-project",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start:be": "./api/node_modules/.bin/nodemon --watch './api/*.ts' --exec 'ts-node' ./api/app.ts",
